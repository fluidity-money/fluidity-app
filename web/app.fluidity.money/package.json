{
  "name": "new.fluidity.money",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prod": "run-s prod:*",
    "prod:build": "cross-env NODE_ENV=production npm run build:server",
    "prod:remix": "cross-env NODE_ENV=production remix build",
    "start": "cross-env NODE_ENV=production PORT=80 node ./build/server.js",
    "dev": "npm-run-all build:remix build:server --parallel dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect -r dotenv/config ./build/server.js",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --external:sharp --bundle",
    "test": "npm-run-all --parallel test:*",
    "test:lint": "eslint --ext .js,.ts,.tsx --max-warnings=0 .",
    "test:style": "prettier -w .",
    "test:unit": "jest --config jest.config.ts --passWithNoTests",
    "test:tsc": "tsc --noEmit",
    "graphql": "npm-run-all graphql:generate graphql:codegen",
    "graphql:generate": "gq --introspect https://fluidity.hasura.app/v1/graphql > schema.graphql",
    "graphql:codegen": "relay-compiler"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/experimental": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@fluidity-money/surfing": "^0.0.95",
    "@openzeppelin/contracts": "^4.7.3",
    "@remix-run/react": "^1.7.1",
    "@remix-run/serve": "^1.7.1",
    "@saberhq/token-utils": "^1.14.9",
    "@sentry/remix": "^7.17.2",
    "@solana/wallet-adapter-base": "^0.9.18",
    "@solana/wallet-adapter-react": "^0.15.23",
    "@solana/wallet-adapter-wallets": "^0.19.5",
    "@solana/web3.js": "^1.63.1",
    "@types/node": "^18.11.2",
    "@types/ws": "^8.5.3",
    "@web3-react/core": "^8.0.35-beta.0",
    "@web3-react/metamask": "^8.0.28-beta.0",
    "@web3-react/types": "^8.0.20-beta.0",
    "@web3-react/walletconnect": "^8.0.35-beta.0",
    "apollo-link": "^1.2.14",
    "apollo-link-ws": "^1.0.20",
    "compression": "^1.7.4",
