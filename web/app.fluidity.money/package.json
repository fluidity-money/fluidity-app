    "framer-motion": "^7.3.6",
    "get-image-colors": "^4.0.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.10.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "quantize": "^1.0.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dnd": "^16.0.1",
